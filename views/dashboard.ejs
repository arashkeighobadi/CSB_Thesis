      <script>
          /* 
              handling space bar detection: 
              For some reason, the input fields don't accept space character.
              Here we add event listener to them and make them accept the space.
          */
          const textInputElements = document.getElementsByClassName("form-control");
          Array.prototype.forEach.call(textInputElements, 
            (element) => {
              element.addEventListener("keydown", (e) => {
                if(e.keyCode == 32) {
                  let val = e.srcElement.value;
                  e.srcElement.value = val + " ";
                }
              });
            }
          );
      </script>
